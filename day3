#include <stdio.h>

int main()
{
    int n,count=0,count1=0,count2=0;
    printf("Enter the number:");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n;i++)
    {
        if(arr[i]==0)
        {
            count++;
            printf("%d ",arr[i]);
        }
        else if(arr[i]==1)
        {
            count1++;
            printf("%d ",arr[i]);
        }
        else
        {
            count2++;
            printf("%d ",arr[i]);
        }
    }
    return 0;
}
