#include <stdio.h>

int main()
{
    char str[50],index;
    fgets(str,sizeof(str),stdin);
   puts(str);
   while(str[index]!='\0')
   {
     index++;  
   }
   printf("%d",index-1);

    return 0;
}
/////////////////////////////////
#include <stdio.h>

int main()
{
    char str[50],index,alpha=0,num=0,spl=0;
    fgets(str,sizeof(str),stdin);
   puts(str);
   while(str[index]!='\0')
   {
       if((str[index]>='a'||str[index]>='A')&&(str[index]<='z'||str[index]<='Z'))
       {
          alpha++; 
       }
       else if((str[index]>='0')&&(str[index]<='9'))
       {
           num++;
       }
       else
       {
           spl++;
       }
       index++;
   }
   printf("%dalpha\n%dnum\n%dspl\n",alpha,num,spl-1);

    return 0;
}
///////////////////////////////////////////////////////////
#include <stdio.h>

int main()
{
    char str[50],index=0,vo=0,cons=0,spl=0;
    fgets(str,sizeof(str),stdin);
   puts(str);
   while(str[index]!='\0')
   {
       if(str[index]=='a'|| str[index]=='A'|| str[index]=='e'|| str[index]=='E'||
       str[index]=='i'|| str[index]=='I'|| str[index]=='o'||str[index]=='O'||
       str[index]=='u'|| str[index]=='U')
       {
           vo++;
       }
       else
       {
           cons++;
       }
       index++;

   }
   printf("%d\n",vo);
   printf("%d",cons-1);
 

    return 0;
}
//////////////////////////////////////////////
#include <stdio.h>

int main()
{
    char str[50],index=0,vo=0,cons=0,spl=0;
    fgets(str,sizeof(str),stdin);
   puts(str);
   while(str[index]!='\0')
   {
       if(str[index]>='a'&&str[index]<='z')
       {
           str[index]=str[index]-32;
           
       }
        printf("%c\n",str[index]);
       index++;
   }
    return 0;
}
