#include <stdio.h>

int main()
{
    char str[50],index=0,vo=0,cons=0,spl=0;
    fgets(str,sizeof(str),stdin);
   puts(str);
   while(str[index]!='\0')
   {
       if(str[index]>='A'&&str[index]<='W')
       {
           str[index]=str[index]+32;
           
       }
       else if(str[index]>='a'&&str[index]<='w')
       {
           str[index]=str[index]-32;
           
       }
       else
       {
           
       }
        printf("%c\n",str[index]);
       index++;
   }
    return 0;
}
////////////////////////////////////////////////////
#include <stdio.h>

int main()
{
    char str[50],index=0,vo=0,cons=0,spl=0;
    fgets(str,sizeof(str),stdin);
   puts(str);
   while(str[index]!='\0')
   {
       if(str[index]==' '||str[index]=='\t'||str[index]=='\n')
       {
           vo++;
           
       }    
   
   index++;
   }
   printf("%d",vo);
    return 0;
}
